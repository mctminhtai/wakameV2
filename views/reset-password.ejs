    <div class="account-login section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 offset-lg-3 col-md-10 offset-md-1 col-12">
                    <form class="card login-form inner-content" method="post">
                        <div class="card-body">
                            <div class="title">
                                <h3>Reset Password</h3>
                                <p>Need to reset your password? No problem! Just enter your email &amp; click the button
                                    below.</p>
                                <% if (message) { %>
                                    <p style="color: brown;">
                                        <%= message %>
                                    </p>
                                    <% } %>
                            </div>
                            <div class="input-head">
                                <div class="form-group input-group">
                                    <label><i class="lni lni-envelope"></i></label>
                                    <input class="form-control" type="email" id="reg-email"
                                        placeholder="Enter your email" name="email" required>
                                </div>
                            </div>
                            <div class="button">
                                <button class="btn" type="submit">Send Reset Link</button>
                                <a class="btn alt" href="accounts/sign-in">Back to Log in</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>


